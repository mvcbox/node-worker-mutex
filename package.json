{
  "name": "worker-mutex",
  "version": "1.0.0",
  "description": "Worker Mutex",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && node ./node_modules/.bin/tsc",
    "clean": "node -e \"['./dist', './coverage', './.nyc_output'].forEach(item => require('fs').rmSync(item, {recursive:true,force:true}));\"",
    "test": "npm run test:unit",
    "test:unit": "mocha --timeout 30000 \"test/**/*.spec.js\"",
    "coverage": "node -r ./test/setup-os-cpus.js ./node_modules/nyc/bin/nyc.js mocha --timeout 30000 \"test/**/*.spec.js\"",
    "prepack": "npm run build"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=18.0.0",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "version": ">=8.6.0",
      "onFail": "error"
    }
  },
  "author": "mvcbox.org@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvcbox/node-worker-mutex/issues"
  },
  "homepage": "https://github.com/mvcbox/node-worker-mutex#readme",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^12.20.55",
    "chai": "^4.3.10",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "typescript": "^5.9.3"
  },
  "nyc": {
    "all": true,
    "concurrency": 1,
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/index.js",
      "dist/main.js",
      "dist/worker.js"
    ],
    "reporter": [
      "text",
      "lcov",
      "html"
    ]
  }
}
